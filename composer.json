{
    "name": "xgproyect/xgproyect",
    "description": "XGP Open-source OGame Clon",
    "keywords": [
        "ogame",
        "clon"
    ],
    "type": "project",
    "homepage": "https://xgproyect.org",
    "support": {
        "source": "https://github.com/XGProyect/XG-Proyect-v3.x.x",
        "issues": "https://github.com/XGProyect/XG-Proyect-v3.x.x/issues"
    },
    "authors": [
        {
            "name": "Lucas Kov√°cs",
            "homepage": "https://github.com/LucasKovacs",
            "role": "Lead"
        }
    ],
    "license": "GPL-3.0-only",
    "require": {
        "php": "^7.3|^8.0",
        "phpmailer/phpmailer": "^6.5",
        "eftec/bladeone": "^4.2"
    },
    "require-dev": {
        "bartlett/php-compatinfo": "^6",
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpcompatinfo": [
            "src/vendor/bin/phpcompatinfo analyser:run ./src"
        ],
        "tests": [
            "src/vendor/bin/phpunit --verbose --config src/tests/phpunit.xml --testsuite xgp_application"
        ]
    },
    "config": {
        "vendor-dir": "src/vendor",
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
